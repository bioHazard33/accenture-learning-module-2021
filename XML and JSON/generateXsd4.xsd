<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<element name="company">
		<complexType>
			<sequence>
				<element maxOccurs="unbounded" name="employee">
					<complexType>
						<sequence>
							<element name="id" type="xs:unsignedByte" />
							<element name="name" type="xs:string" />
							<element name="salary" type="xs:unsignedShort" />
							<element name="email" type="xs:string" />
						</sequence>
					</complexType>
				</element>
			</sequence>
		</complexType>
	</element>
</schema>
